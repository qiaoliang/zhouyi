    "seed:hexagrams:clean": "ts-node -r tsconfig-paths/register scripts/seed-hexagrams.ts clean"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/axios": "^4.0.1",
